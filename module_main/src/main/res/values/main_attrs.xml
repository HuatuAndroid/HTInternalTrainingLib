<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Main_RatingBar">
        <!--尺寸值-->
        <attr name="main_starImageSize" format="dimension" />
        <!--星星间距-->
        <attr name="main_starPadding" format="dimension" />
        <!--星星总数-->
        <attr name="main_starCount" format="integer" />
        <!--空白的星星资源文件值-->
        <attr name="main_starEmpty" format="reference" />
        <!--满星资源文件值-->
        <attr name="main_starFill" format="reference" />
        <!--半星资源文件值-->
        <attr name="main_starHalf" format="reference" />
        <!--是否可点击boolean值-->
        <attr name="main_clickable" format="boolean" />
        <!--当前进度float值-->
        <attr name="main_starStep" format="float" />
        <!--每次进度方式的值，整星还是半星-->
        <attr name="main_stepSize">
            <enum name="Half" value="0" />
            <enum name="Full" value="1" />
        </attr>
    </declare-styleable>
</resources>